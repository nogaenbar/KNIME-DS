{
  "base": {
    "fg": {
      "default": {
        "$type": "color",
        "$value": "#"
      }
    },
    "bg": {
      "default": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "darken",
              "value": "0.2",
              "space": "lch"
            }
          }
        },
        "$type": "color",
        "$value": "{core.color.yellow.50}"
      }
    },
    "selectable": {
      "primary": {
        "unselected": {
          "enabled": {
            "bg": {
              "$type": "color",
              "$value": "{core.color.transparent}"
            }
          },
          "hover": {
            "bg": {
              "$type": "color",
              "$value": "{core.color.transparent}"
            }
          },
          "pressed": {
            "bg": {
              "$type": "color",
              "$value": "{core.color.transparent}"
            }
          }
        },
        "selected": {
          "enabled": {
            "bg": {
              "$type": "color",
              "$value": "{core.color.transparent}"
            }
          },
          "hover": {
            "bg": {
              "$type": "color",
              "$value": "{core.color.transparent}"
            }
          },
          "pressed": {
            "bg": {
              "$type": "color",
              "$value": "{core.color.transparent}"
            }
          }
        }
      },
      "success": {
        "canvas-bg": {
          "$type": "color",
          "$value": "{core.color.green.60}"
        }
      },
      "error": {
        "default": {
          "$type": "color",
          "$value": "{core.color.red.40}"
        }
      }
    }
  }
}