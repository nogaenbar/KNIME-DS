{
  "interactive": {
    "fg": {
      "default": {
        "$type": "color",
        "$value": "{core.color.neutral.10}"
      },
      "disabled": {
        "$type": "color",
        "$value": "{core.color.neutral.40}"
      }
    },
    "bg": {
      "default": {
        "$type": "color",
        "$value": "{core.color.yellow.50}"
      },
      "disabled": {
        "$type": "color",
        "$value": "{core.color.neutral.70}"
      }
    },
    "primary": {
      "enabled": {
        "bg": {
          "$type": "color",
          "$value": "{interactive.bg.default}"
        }
      },
      "hover": {
        "bg": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "lighten",
                "value": ".16",
                "space": "lch"
              }
            }
          },
          "$type": "color",
          "$value": "{interactive.bg.default}"
        }
      },
      "focus": {
        "bg": {
          "$type": "color",
          "$value": "{interactive.bg.default}"
        }
      },
      "disabled": {
        "bg": {
          "$type": "color",
          "$value": "{interactive.bg.disabled}"
        }
      }
    }
  },
  "base": {
    "color": {
      "modifiers": {
        "enabled-alpha": {
          "$type": "other",
          "$value": "1"
        },
        "disabled-alpha": {
          "$type": "other",
          "$value": "0.5"
        },
        "lightness": {
          "$type": "other",
          "$value": "0.1"
        },
        "button": {
          "focused": {
            "$type": "other",
            "$value": "0.5"
          },
          "hover": {
            "$type": "other",
            "$value": "{base.color.modifiers.lightness}"
          },
          "pressed": {
            "$type": "other",
            "$value": "{base.color.modifiers.lightness}"
          }
        }
      }
    }
  },
  "core": {
    "box-shadow": {
      "intensity": {
        "$type": "other",
        "$value": "0.1"
      }
    }
  }
}