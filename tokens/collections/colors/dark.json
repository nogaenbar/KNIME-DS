{
  "interactive": {
    "button": {
      "fg": {
        "default": {
          "$type": "color",
          "$value": "{core.color.neutral.90}"
        },
        "default-inverted": {
          "$type": "color",
          "$value": "{core.color.neutral.10}"
        },
        "subtle": {
          "$type": "color",
          "$value": "{core.color.neutral.50}"
        },
        "strong": {
          "$type": "color",
          "$value": "{core.color.neutral.60}"
        }
      },
      "bg": {
        "default": {
          "$type": "color",
          "$value": "{core.color.yellow.50}"
        },
        "none": {
          "$type": "color",
          "$value": "{core.color.transparent}"
        },
        "primary": {
          "enabled": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "darken",
                  "value": ".04",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{interactive.button.bg.default}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "{interactive.color.modifiers.states.hover.light}",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{interactive.button.bg.default}"
          },
          "focused": {
            "$type": "color",
            "$value": "{interactive.button.bg.default}"
          },
          "disabled": {
            "$type": "color",
            "$value": "{core.color.neutral.30}"
          }
        },
        "secondary": {
          "enabled": {
            "$type": "color",
            "$value": "{interactive.button.bg.none}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "{interactive.color.modifiers.hover-alpha}",
                  "space": "lch",
                  "color": "{core.color.neutral.10}"
                }
              }
            },
            "$type": "color",
            "$value": "{core.color.neutral.30}"
          },
          "focused": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "{interactive.color.modifiers.button.focused}",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{interactive.button.bg.none}"
          },
          "disabled": {
            "$type": "color",
            "$value": "{interactive.button.bg.none}"
          }
        },
        "tertiary": {
          "enabled": {
            "$type": "color",
            "$value": "{interactive.button.bg.none}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "{interactive.color.modifiers.hover-alpha}",
                  "space": "lch",
                  "color": "{core.color.neutral.10}"
                }
              }
            },
            "$type": "color",
            "$value": "{core.color.neutral.30}"
          },
          "focused": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "{interactive.color.modifiers.button.focused}",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{interactive.button.bg.none}"
          },
          "disabled": {
            "$type": "color",
            "$value": "{interactive.button.bg.none}"
          }
        }
      }
    },
    "color": {
      "modifiers": {
        "hover-alpha": {
          "$type": "other",
          "$value": "0.8"
        },
        "lightness": {
          "$type": "other",
          "$value": "0.16"
        },
        "button": {
          "hover": {
            "$type": "other",
            "$value": "{interactive.color.modifiers.lightness}"
          },
          "pressed": {
            "$type": "other",
            "$value": "{interactive.color.modifiers.lightness}"
          },
          "focused": {
            "$type": "other",
            "$value": "0"
          }
        }
      }
    }
  },
  "core": {
    "box-shadow": {
      "intensity": {
        "$type": "other",
        "$value": "0.1"
      }
    }
  },
  "border": {
    "default": {
      "$type": "border",
      "$value": {
        "color": "{core.color.neutral.70}",
        "width": "{base.border-width.s}",
        "style": "solid"
      }
    },
    "subtle": {
      "$type": "border",
      "$value": {
        "color": "{core.color.neutral.40}",
        "width": "{base.border-width.s}",
        "style": "solid"
      }
    },
    "brand": {
      "$type": "border",
      "$value": {
        "color": "{core.color.yellow.40}",
        "width": "{base.border-width.s}",
        "style": "solid"
      }
    }
  }
}